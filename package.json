{
  "name": "css-literal",
  "version": "0.0.1",
  "description": "Write CSS objects as template literals",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/benjaminj6/css-literal.git"
  },
  "keywords": [
    "css-in-js",
    "css",
    "template-literal",
    "javascript"
  ],
  "author": "Benjamin Johnson <benjamin.d.johnson@icloud.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benjaminj6/css-literal/issues"
  },
  "homepage": "https://github.com/benjaminj6/css-literal#readme",
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.5.1",
    "babel-register": "^6.24.1",
    "nyc": "^11.0.2",
    "prettier": "^1.4.2",
    "rollup": "^0.42.0",
    "rollup-plugin-babel": "^2.7.1"
  },
  "babel": {
    "presets": [
      "env"
    ]
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": "inherit"
  },
  "scripts": {
    "format": "prettier src/**/*.js --semi=false --single-quote --write",
    "build": "rollup -c",
    "test": "ava",
    "coverage": "nyc -a npm test"
  }
}
